#import "MNBaseIconBtn.h"

@implementation MNBaseIconBtn

- (instancetype)init
{
    self = [super init];
    if (self) {
        self.layer.cornerRadius = self.frame.size.width/2.f;
        self.clipsToBounds = YES;
    }
    return self;
}

-(void)awakeFromNib
{
    self.layer.cornerRadius = self.frame.size.width/2.f;
    self.clipsToBounds = YES;
}

- (void)setHighlighted:(BOOL)highlighted {}

@end
