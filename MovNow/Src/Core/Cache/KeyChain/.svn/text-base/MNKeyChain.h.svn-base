#import <Foundation/Foundation.h>

@interface MNKeyChain : NSObject
/**
 *  保存
 *
 *  @param service 名称
 *  @param data    内容
 */
+ (void)save:(NSString *)service data:(id)data;
/**
 *  加载
 *
 *  @param service 名称
 *
 *  @return 内容
 */
+ (id)load:(NSString *)service;
/**
 *  删除
 *
 *  @param service 名称
 */
+ (void)delete:(NSString *)service;
@end
