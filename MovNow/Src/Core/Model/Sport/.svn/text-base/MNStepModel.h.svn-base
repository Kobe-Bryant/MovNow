#import <Foundation/Foundation.h>
#import "JSONModel.h"
/**
 *  计步模型
 */
@interface MNStepModel : JSONModel
@property (nonatomic,readonly,copy) NSNumber *userId;  //用户id
@property (nonatomic,readonly,copy) NSString *deviceId; //手环设备唯一识别码
@property (nonatomic,copy) NSString *sportTime; //运动时间  格式"yyyyMMddHHmmss"
